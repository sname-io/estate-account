{
  "name": "estate-account",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "db:migrate": "npx sequelize db:migrate",
    "db:create": "npx sequelize db:create",
    "db:seed": "npx sequelize db:seed:all",
    "db:seed:undo": "npx sequelize db:seed:undo",
    "migrate:reset": "npx sequelize db:migrate:undo:all && npm run db:migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test:prepare": "cross-env NODE_ENV=test npm run db:create",
    "admin:create": "node ./tasks/setup-admin"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bcrypt": "^5.0.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "currency-formatter": "^1.5.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-dynamic-helpers-patch": "0.0.2",
    "express-flash": "git://github.com/RGBboy/express-flash.git",
    "express-paginate": "^1.0.2",
    "express-session": "^1.17.1",
    "faker": "^4.1.0",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "moment": "^2.27.0",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pug": "3.0.1",
    "secure-random-string": "^1.1.3",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.0",
    "uuidv4": "^6.2.1"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "jest": "^26.1.0",
    "supertest": "^4.0.2"
  }
}
